

<%- include("navbar.ejs", {searchBar:false, searched: ''})%>

<div class="container">
  <div class="trends-div">
    <h1 class="trend-title">TRENDING NEWS</h1>
      <% allArticles.forEach((article, index) => {%>
      <div class="row">
        <div class="col-12">
          <div class="card trend-card">
            <div class="card-body d-flex">
                <div class="col-md-1 me-2"><p class="rank-num text-body-secondary"><%=index + 1%></p></div>
              <div class="col-md-9">
                <h2 class="card-title">
                    <%=article.title%></h2>
              </div>
            </div>
          </div>
        </div>
      </div>
      <%});%>
      
  </div>
</div>
